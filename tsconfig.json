{
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noImplicitAny": false,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@core/*": [
        "./src/core/*"
      ],
      "@api-client": [
        "./src/core/api-client/index"
      ],
      "@icons": [
        "./src/core/icons/index"
      ],
      "@components/*": [
        "./src/core/components/*"
      ],
      "@utils/*": [
        "./src/core/utils/*"
      ],
      "@features/*": [
        "./src/features/*"
      ]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts"
, "src/features/auth/forgot/forgot-password.ts"  ],
  "exclude": [
    "node_modules"
  ]
}
